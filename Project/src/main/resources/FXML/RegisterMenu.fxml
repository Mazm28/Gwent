<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.shape.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" stylesheets="@../CSS/MainStyle.css" xmlns="http://javafx.com/javafx/17.0.2-ea" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.RegisterMenu">
   <children>
      <ImageView fitHeight="700" fitWidth="1000">
         <image>
            <Image url="@../IMAGES/RegisterMenuBackGround.JPG" />
         </image>
      </ImageView>
      <VBox layoutX="330.0" layoutY="130.0" prefHeight="460.0" prefWidth="369.0" styleClass="login-menu-vbox" />
      <VBox alignment="CENTER" layoutX="331.0" layoutY="147.0" prefHeight="448.0" prefWidth="339.0" spacing="15.0" stylesheets="@../CSS/MainStyle.css">
         <children>
            <Label fx:id="error" textFill="#9e0008" />
            <TextField fx:id="username" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="35.0" prefWidth="290.0" promptText="username" styleClass="tf_box">
               <font>
                  <Font size="17.0" />
               </font>
            </TextField>
            <TextField fx:id="email" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="35.0" prefWidth="290.0" promptText="email" styleClass="tf_box">
               <font>
                  <Font size="17.0" />
               </font>
            </TextField>
            <TextField fx:id="nickname" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="35.0" prefWidth="290.0" promptText="nickname" styleClass="tf_box">
               <font>
                  <Font size="17.0" />
               </font>
            </TextField>
            <Pane prefHeight="40.0" prefWidth="339.0">
               <children>
                  <PasswordField fx:id="password" alignment="CENTER" layoutX="25.0" layoutY="3.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="35.0" prefWidth="290.0" promptText="password" styleClass="tf_box">
                     <font>
                        <Font size="17.0" />
                     </font>
                  </PasswordField>
                  <TextField fx:id="username1" alignment="CENTER" disable="true" layoutX="25.0" layoutY="3.0" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" prefHeight="35.0" prefWidth="290.0" promptText="password" styleClass="tf_box" visible="false">
                     <font>
                        <Font size="17.0" />
                     </font>
                  </TextField>
                  <Ellipse onMouseClicked="#showPassword" fx:id="showCircle" fill="#1f93ff00" layoutX="296.0" layoutY="20.0" radiusX="14.0" radiusY="12.0" stroke="#26181800" strokeType="INSIDE">
                     <cursor>
                        <Cursor fx:constant="HAND" />
                     </cursor></Ellipse>
               </children>
            </Pane>
            <TextField fx:id="repeatPassword" alignment="CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" onKeyPressed="#signUp2" prefHeight="35.0" prefWidth="290.0" promptText="repeat password" styleClass="tf_box">
               <font>
                  <Font size="17.0" />
               </font>
            </TextField>
            <Button fx:id="signIn" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#signIn" prefHeight="35.0" prefWidth="166.0" styleClass="button-52" stylesheets="@../CSS/MainStyle.css" text="Sign In" textFill="WHITE">
               <font>
                  <Font size="10.0" />
               </font>
            </Button>
            <Button fx:id="signUp" alignment="TOP_CENTER" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#signUp" prefHeight="35.0" prefWidth="168.0" styleClass="button-52" text="Sign Up" textFill="WHITE" />
            <Button fx:id="randomPassword" maxHeight="-Infinity" maxWidth="-Infinity" minHeight="-Infinity" minWidth="-Infinity" mnemonicParsing="false" onMouseClicked="#makePassword" prefHeight="35.0" prefWidth="166.0" styleClass="button-52" stylesheets="@../CSS/MainStyle.css" text="Random Password" textFill="WHITE">
               <font>
                  <Font size="10.0" />
               </font>
            </Button>
         </children>
      </VBox>
   </children>
</AnchorPane>
